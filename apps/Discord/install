cd $HOME
wget https://github.com/oxmc/disweb/releases/download/Latest/DisWeb-armv7l.AppImage
 echo "[Desktop Entry]
Name=DisWeb
Comment=A Discord client for raspberry pi's
Exec=./$HOME/DisWeb-armv7l.AppImage
Icon=$HOME/.config/app.png
Categories=Utility;
Type=Application
Terminal=false" > $HOME/.local/share/applications/DisWeb.desktop
